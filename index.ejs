<!doctype html>
<html lang="zh">
  <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width,initial-scale=1">
      <title><%= htmlWebpackPlugin.options.title %></title>
      <style type="text/css">
      .index-loading {
        position: absolute;
        top: 30%;
        width: 100%;
        text-align: center;
        display: none;
        margin-top: -20px;
      }
      #react:empty+.index-loading {
        display: inherit;
      }
      .loading{
        width: 80px;
        height: 40px;
        margin: 0 auto;
        margin-top:100px;
      }
      .loading span{
        display: inline-block;
        width: 8px;
        height: 100%;
        border-radius: 4px;
        background: lightgreen;
        -webkit-animation: load 1s ease infinite;
      }
      @-webkit-keyframes load{ 
        0%,100%{
          height: 40px;
          background: lightgreen;
        }
        50%{
          height: 70px;
          margin: -15px 0;
          background: lightblue;
        }
      }
      .loading span:nth-child(2){ -webkit-animation-delay:0.2s; }
      .loading span:nth-child(3){ -webkit-animation-delay:0.4s; }
      .loading span:nth-child(4){ -webkit-animation-delay:0.6s; }
      .loading span:nth-child(5){ -webkit-animation-delay:0.8s; }
      </style>
      <!--[if lt IE 10]>
      <script src="https://as.alipayobjects.com/g/component/??console-polyfill/0.2.2/index.js,es5-shim/4.5.7/es5-shim.min.js,es5-shim/4.5.7/es5-sham.min.js,html5shiv/3.7.2/html5shiv.min.js,media-match/2.0.2/media.match.min.js"></script>
      <![endif]-->
  </head>
  <body>
      <div id="react" class="react"></div>
      <div class="index-loading">
        <div class="loading">
          <span></span>
          <span></span>
          <span></span>
          <span></span>
          <span></span>
        </div>
      </div>
  </body>
</html>
